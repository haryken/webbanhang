
<!-- SECTION -->
<div class="section">
	<!-- container -->
	<div class="container">
		<!-- row -->
    <div id="store" class="col-md-12">
			<!-- store products -->
			<div class="row">
				<!-- Product tab -->
						<div class="col-md-12">
								<!-- product tab nav -->
								<ul class="tab-nav">
									<li class="active" style="display: inline;"><a data-toggle="tab" href="#tab1">SAMSUNG</a></li>
									<li style="display: inline;padding-left: 15px"><a data-toggle="tab" href="#tab2">SONY</a></li>
									<li style="display: inline;padding-left: 15px"><a data-toggle="tab" href="#tab3">LG</a></li>
								</ul>
								<!-- /product tab nav -->

								<!-- product tab content -->
								<div class="tab-content">
									<!-- tab1  -->
									<div id="tab1" class="tab-pane fade in active">
										<%for(i=0;i<length;i++){
											if(products[i].company=='samsung'){
											%>
										<!-- product -->
										<form class="" action="/thukho" method="post">
											<div class="col-md-3 col-xs-6">
												<div class="product">
													<div class="product-img">
														<img src="<%=products[i].picture%>" width="50px" height="200px" alt="">
														<div class="product-label">
															<span class="sale">-<%=products[i].discount%></span>
														</div>
													</div>
													<div class="product-body" style="height: 170px">
														<p class="product-category"><%=products[i].type%> <%=products[i].company%></p>
														<h3 class="product-name"><%=products[i].name%></h3>
														<h4 class="product-price"><%=new Intl.NumberFormat().format(products[i].price)%> VNĐ</h4>
														<div class="product-btns">
															<button class="add-to-compare" name="submit" value="custom"><i class="fa fa-cog"></i><span class="tooltipp">Custom</span></button>
															<button class="quick-view" name="submit" value="view"><i class="fa fa-eye"></i><span class="tooltipp">quick view</span></button>
														</div>
													</div>
													<input type="hidden" name="id" value="<%=products[i].id%>">
													<input type="hidden" name="amount" value="1">
													<input type="hidden" name="color" value="<%=products[i].color%>">
													<input type="hidden" name="company" value="<%=products[i].company%>">
													<input type="hidden" name="discount" value="<%=products[i].discount%>">
													<input type="hidden" name="info" value="<%=products[i].info%>">
													<input type="hidden" name="name" value="<%=products[i].name%>">
													<input type="hidden" name="picture" value="<%=products[i].picture%>">
													<input type="hidden" name="price" value="<%=products[i].price%>">
													<input type="hidden" name="size" value="<%=products[i].size%>">
													<input type="hidden" name="type" value="<%=products[i].type%>">
													<input type="hidden" name="state" value="<%=products[i].state%>">
												</div>
											</div>
										</form>
										<!-- /product -->

										<div class="clearfix visible-sm visible-xs"></div>
