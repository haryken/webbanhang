											if(products[i].vaitro=='1'){
											%>
                      <!-- product -->
                      <form class="" action="/quanly" method="post">
                        <div class="col-md-3 col-xs-6">
                          <div class="product">
                            <div class="product-body" style="height: 170px">
                              <h3 class="product-name">Name: <%=products[i].name%></h3>
                              <p><b>Username</b> <%=products[i].username%></p>
                              <p><b>Email</b> <%=products[i].mail%></p>
                              <p><b>Phone</b> <%=products[i].phone%></p>
                              <div class="product-btns">
                                <button class="add-to-compare" name="submit" value="custom"><i class="fa fa-cog"></i><span class="tooltipp">Custom</span></button>
                              </div>
                            </div>
                          </div>
                        </div>
                        <input type="hidden" name="username" value="<%=products[i].username%>">
                      </form>
                      <!-- /product -->

										<div class="clearfix visible-sm visible-xs"></div>
