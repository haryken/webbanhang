				<!-- Product tab -->
						<div class="col-md-12">
								<!-- product tab nav -->
								<ul class="tab-nav">
									<li class="active" style="display: inline;"><a data-toggle="tab" href="#tab1">Saler</a></li>
									<li style="display: inline;padding-left: 15px"><a data-toggle="tab" href="#tab2">Warehouse manager</a></li>
